{
  "name": "duckfeed",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "set PORT=3005 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server-start": "node ./server/server",
    "server-test": "mocha test/server/**/*.js",
    "dev-start": "concurrently --kill-others-on-fail \"nodemon ./server/server\" \"npm run start\""
  },
  "dependencies": {
    "body-parser": "~1.12.4",
    "bootstrap": "^4.1.3",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.12.4",
    "mongoose": "^4.4.19",
    "morgan": "~1.5.3",
    "nodemon": "^1.17.5",
    "react": "^16.3.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "save-dev": "^2.0.0",
    "serve-favicon": "~2.2.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "concurrently": "^3.6.1",
    "mocha": "^5.2.0"
  }
}
