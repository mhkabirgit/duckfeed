{
  "name": "duckfeed",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "set PORT=3005 && react-scripts start",
    "build": "react-scripts build",
    "test-schedule": "node ./server/service/scheduleThread",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server-start": "node ./server/server",
    "server-test": "mocha --timeout 5000 test/server/**/*.js && node ./server/server",
    "dev-start": "concurrently --kill-others-on-fail \"nodemon ./server/server\" \"npm run start\""
  },
  "dependencies": {
